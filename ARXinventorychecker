local request = (syn and syn.request) or http_request or request or (http and http.request)
if not request then return warn("âŒ Executor à¸™à¸µà¹‰à¹„à¸¡à¹ˆà¸£à¸­à¸‡à¸£à¸±à¸šà¸à¸²à¸£à¸ªà¹ˆà¸‡ Webhook") end

local webhook_url = "https://discord.com/api/webhooks/1248392620162547754/4M53Za1YDAiIUBLXIQywo64JFUel-obq-HkRBM3KdDWqkF-Ou4cHfKkk6ydj_lOT_4sV"

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local itemsFolder = ReplicatedStorage:WaitForChild("Player_Data"):WaitForChild(player.Name):WaitForChild("Items")

local emojiMap = {
    ["Alpha Dust"] = "âœ¨",
    ["Ancient Power Core"] = "ğŸ”‹",
    ["Ancient Talisman"] = "ğŸ§¿",
    ["Arata Armor Piece"] = "ğŸ›¡ï¸",
    ["Arcane Dust"] = "ğŸ”®",
    ["Blood Ember"] = "ğŸ”¥",
    ["CGGG Investigator's Badge"] = "ğŸ–ï¸",
    ["Chakra Shard"] = "ğŸ’ ",
    ["Chaos Bone Shard"] = "ğŸ¦´",
    ["Chaotic Sword"] = "âš”ï¸"
}

task.wait(2)

local description = ""
local foundItems = 0

for _, itemFolder in ipairs(itemsFolder:GetChildren()) do
    local amountValue = itemFolder:FindFirstChild("Amount")
    if amountValue and amountValue:IsA("IntValue") then
        local itemName = itemFolder.Name
        local amount = amountValue.Value
        local emoji = emojiMap[itemName] or "ğŸ”¹"
        local line = string.format("%s **%s** â€” `x%d`\n", emoji, itemName, amount)
        
        -- à¸•à¸±à¸”à¸–à¹‰à¸²à¹€à¸à¸´à¸™ Discord Embed limit (à¸›à¸£à¸°à¸¡à¸²à¸“ 2048 à¸•à¸±à¸§à¸­à¸±à¸à¸©à¸£)
        if #description + #line < 2000 then
            description = description .. line
            foundItems += 1
        end
    end
end

if foundItems == 0 then
    description = "âš ï¸ à¹„à¸¡à¹ˆà¸¡à¸µà¹„à¸­à¹€à¸—à¸¡à¹ƒà¸™à¸•à¸±à¸§à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™"
end

local payload = HttpService:JSONEncode({
    embeds = {{
        title = "ğŸ“¦ Inventory Report for " .. player.Name,
        description = description,
        color = 0x00ff99,
        footer = { text = "âœ… à¸ªà¹ˆà¸‡à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¹€à¸£à¸µà¸¢à¸šà¸£à¹‰à¸­à¸¢à¹à¸¥à¹‰à¸§!" }
    }}
})

local success, err = pcall(function()
    request({
        Url = webhook_url,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = payload
    })
end)

if success then
    print("âœ… à¸ªà¹ˆà¸‡à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¸ªà¸³à¹€à¸£à¹‡à¸ˆ")
else
    warn("âŒ à¸ªà¹ˆà¸‡à¹„à¸¡à¹ˆà¸ªà¸³à¹€à¸£à¹‡à¸ˆ:", err)
end
