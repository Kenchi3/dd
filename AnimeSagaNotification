-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö executor
local request = (syn and syn.request) or http_request or request or (http and http.request)
if not request then
    return warn("‚ùå Executor ‡∏ô‡∏µ‡πâ‡πÑ‡∏°‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏™‡πà‡∏á Webhook")
end

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Webhook URL
local webhook_url = "https://discord.com/api/webhooks/1254548069144461382/yKMM0gt9d7cg1Ruc7WjENOMQDluII9kMIw_LlfJJc_C4fNJdaqgJ6T3tdAZ56bIYcwtS" -- <== ‡πÅ‡∏Å‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì

-- ‡∏î‡∏∂‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local playerData = ReplicatedStorage:WaitForChild("PlayerData"):WaitForChild(player.Name)
local itemBag = playerData:WaitForChild("Inventory"):WaitForChild("ItemBag")
local items = HttpService:JSONDecode(itemBag.Value)

-- ‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡∏Å‡∏≥‡∏´‡∏ô‡∏î emoji ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏•‡∏∞ Item
local emojiMap = {
    ["Demon Blood"] = "ü©∏",
    Kimbap = "üçô",
    Spell = "üìú",
    SuperSpell = "üåü",
    Bottle = "üçæ",
    Ticket = "üé´",
    Gear = "‚öôÔ∏è",
    Milk = "ü•õ",
    Screws = "üî©",
    Yoyo = "ü™Ä",
    Egg = "ü•ö",
    Handheld = "üéÆ",
    Chip = "üíæ",
    Battery = "üîã",
    Dice = "üé≤",
    Key = "üîë",
    TraitReroll = "üß¨",
    Narutomaki = "üç•",
    Onigiri = "üçô",
    ["Meat Bone"] = "üçñ",
    Udon = "üçú",
    Magnet = "üß≤",
    Soup = "üç≤"
}


-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
local msg = "üì¶ **Inventory Report for `" .. player.Name .. "`**\n"
msg = msg .. "‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\n"

for _, item in ipairs(items) do
    local emoji = emojiMap[item.Items] or "üîπ" -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÄ‡∏à‡∏≠ emoji ‡πÉ‡∏ä‡πâ üîπ ‡πÅ‡∏ó‡∏ô
    msg = msg .. emoji .. " **" .. item.Items .. "** ‚Äî `x" .. item.Number .. "`\n"
end

msg = msg .. "‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\n‚úÖ **‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ñ‡∏π‡∏Å‡∏™‡πà‡∏á‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!**"

-- ‡πÄ‡∏ï‡∏£‡∏µ‡∏¢‡∏° Payload
local payload = HttpService:JSONEncode({
    content = msg
})

-- ‡∏™‡πà‡∏á Webhook
local success, err = pcall(function()
    request({
        Url = webhook_url,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = payload
    })
end)

-- ‡∏£‡∏≤‡∏¢‡∏á‡∏≤‡∏ô‡∏ú‡∏•
if success then
    print("‚úÖ ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à")
else
    warn("‚ùå ‡∏™‡πà‡∏á‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: " .. tostring(err))
end
